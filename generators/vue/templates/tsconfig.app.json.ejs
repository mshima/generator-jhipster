{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": ["env.d.ts", "src/**/*", "src/**/*.vue"],
  "exclude": ["src/**/__tests__/*"],
  "compilerOptions": {
    "composite": true,
    "baseUrl": "<%= clientSrcDir %>app",
    "outDir": "<%= temporaryDir %>out-tsc/dist",
    "paths": {
      "@/*": ["./src/*"]
    }
  }
}
